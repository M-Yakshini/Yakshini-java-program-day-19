class Solution {
    public List<Integer> postorderTraversal(TreeNode r) {
        LinkedList<Integer> a = new LinkedList<>();
        Stack<TreeNode> s = new Stack<>();
        if (r != null) s.push(r);
        while (!s.isEmpty()) {
            TreeNode t = s.pop();
            a.addFirst(t.val);
            if (t.left != null) s.push(t.left);
            if (t.right != null) s.push(t.right);
        }
        return a;
    }
}
